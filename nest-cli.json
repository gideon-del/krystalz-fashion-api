{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/store/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/store/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/store",
  "projects": {
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "store": {
      "type": "application",
      "root": "apps/store",
      "entryFile": "main",
      "sourceRoot": "apps/store/src",
      "compilerOptions": {
        "tsConfigPath": "apps/store/tsconfig.app.json"
      }
    },
    "product": {
      "type": "library",
      "root": "libs/product",
      "entryFile": "index",
      "sourceRoot": "libs/product/src",
      "compilerOptions": {
        "tsConfigPath": "libs/product/tsconfig.lib.json"
      }
    },
    "category": {
      "type": "library",
      "root": "libs/category",
      "entryFile": "index",
      "sourceRoot": "libs/category/src",
      "compilerOptions": {
        "tsConfigPath": "libs/category/tsconfig.lib.json"
      }
    }
  }
}